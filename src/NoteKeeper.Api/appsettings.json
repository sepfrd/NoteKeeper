{
  "AllowedHosts": "*",
  "BaseApiUrl": "http://localhost:5000/",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "DatabaseConnectionString": "Server=localhost;Port=5432;Database=NoteKeeper;User Id=postgres;Password=;Trust Server Certificate=True",
  "RedisOptions": {
    "Endpoint": "localhost",
    "Port": 6379,
    "User": "default",
    "Password": "",
    "ConnectTimeout": 5000,
    "UseSsl": false,
    "ClientName": "NoteKeeper",
    "RetryAttempts": 5,
    "KeepAlive": 180,
    "Databases": {
      "Default": 0,
      "Notes": 0,
      "Auth": 1
    }
  },
  "JwtOptions": {
    "PrivateKey": "",
    "PublicKey": "",
    "Issuer": "NoteKeeper Server Application",
    "Audience": "NoteKeeper Client Application",
    "TokenLifetimeInSeconds": 86400,
    "RefreshTokenLifeTimeInMinutes": 10080
  },
  "GoogleOAuthOptions": {
    "RedirectUri": "https://redirectmeto.com/http://localhost:8000/api/oauth/v2/google/code",
    "ClientId": "{GoogleClientId}",
    "ClientSecret": "{GoogleClientSecret}",
    "ProjectId": "{GoogleProjectId}",
    "AuthUri": "https://accounts.google.com/o/oauth2/auth",
    "TokenUri": "https://oauth2.googleapis.com/token",
    "RevokeUri": "https://oauth2.googleapis.com/revoke",
    "AuthProviderX509CertUrl": "https://www.googleapis.com/oauth2/v1/certs"
  },
  "NotionOAuthOptions": {
    "RedirectUri": "https://redirectmeto.com/http://localhost:8000/api/oauth/v2/notion/code",
    "ClientId": "{NotionClientId}",
    "ClientSecret": "{NotionClientSecret}",
    "AuthUri": "https://api.notion.com/v1/oauth/authorize",
    "TokenUri": "https://api.notion.com/v1/oauth/token"
  },
  "HttpClientOptions": {
    "DefaultTimeoutInMilliseconds": 10000
  },
  "CorsOptions": {
    "AllowedUrls": [
      "http://localhost:3000",
      "http://localhost:5000",
      "https://sepfrd.com"
    ]
  }
}
